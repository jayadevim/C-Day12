#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    for(int i = 0; i < n; i++) {          
        for(int j = 0; j < n; j++) {      
            if ((i + j) % 2 == 0)
                printf("1");
            else
                printf("0");
        }
        printf("\n");
    }

    return 0;
}

OUTPUT:
    9
101010101
010101010
101010101
010101010
101010101
010101010
101010101
010101010
101010101
